  - respuesta: 800.000 + 5n
    correcta: 0
  - respuesta: 800.000 (5n)
    correcta: 0
  - respuesta: 800.000*(5/100)^n
    correcta: 0
  - respuesta: 800.000*(1 + (5/100))^n
    correcta: 1
